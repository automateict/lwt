<%= simple_form_for(@petition_review_member) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :government_body %>
    <%= f.input :name %>
    <%= f.input :profession %>
    <%= f.input :phone %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
