<p id="notice"><%= notice %></p>

<h1>Pr Committee Members</h1>

<table>
  <thead>
    <tr>
      <th>Pr committee</th>
      <th>Title</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Profession</th>
      <th>Organization</th>
      <th>About the member</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pr_committee_members.each do |pr_committee_member| %>
      <tr>
        <td><%= pr_committee_member.pr_committee %></td>
        <td><%= pr_committee_member.title %></td>
        <td><%= pr_committee_member.first_name %></td>
        <td><%= pr_committee_member.last_name %></td>
        <td><%= pr_committee_member.email %></td>
        <td><%= pr_committee_member.phone %></td>
        <td><%= pr_committee_member.profession %></td>
        <td><%= pr_committee_member.organization %></td>
        <td><%= pr_committee_member.about_the_member %></td>
        <td><%= link_to 'Show', pr_committee_member %></td>
        <td><%= link_to 'Edit', edit_pr_committee_member_path(pr_committee_member) %></td>
        <td><%= link_to 'Destroy', pr_committee_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr Committee Member', new_pr_committee_member_path %>
