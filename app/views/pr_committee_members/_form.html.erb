<%= simple_form_for(@pr_committee_member) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :pr_committee %>
    <%= f.input :title %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :profession %>
    <%= f.input :organization %>
    <%= f.input :about_the_member %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
